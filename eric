# # x = 10 
# # x = x + 10 
# # print(x)

def digitCount(number): 
	count = 0
	while number > 0: 	 
		number = number // 10 
		count = count + 1 
	return count 

# print(digitCount(91))


def digitCountForLoop(number): 
	count = 0
	for i in range(0, number): 	 
		number = number // 10 
		count = count + 1 
		if number <= 0: 
			 break
	return count 

# print(digitCountForLoop(312))



