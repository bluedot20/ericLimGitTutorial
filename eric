# print(digitCount2(123))


def reverseDigit(number): 
	digitCount = 0 
	copyNum = number 
	while copyNum > 0: 
		copyNum = copyNum // 10 
		digitCount = digitCount + 1 
	answer = 0 
	while number > 0: 
		digit = number % 10
		number = number // 10 
		answer = answer + digit * 10 ** (digitCount - 1)
		digitCount = digitCount - 1 
	return answer 

print(reverseDigit(12345))


def oppositeNum(n):
	answer = 0 
	while (n > 0): 
		digit = n % 10 
		answer = answer * 10 + digit 
		n = n // 10 
	return answer 

print(oppositeNum(123))







